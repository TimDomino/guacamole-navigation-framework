<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE setup SYSTEM "config.dtd">

<setup>

  <global>

    <animatecoupling>True</animatecoupling>

  </global>

  <device>

    <type>Spacemouse</type>

    <inputsensor>device-spacemouse</inputsensor>

    <display>small_powerwall</display>

    <avatartype>joseph</avatartype>

    <transmitteroffset>
      <x>0.0</x>
      <y>0.043</y>
      <z>1.6</z>
      <rx>0.0</rx>
      <ry>0.0</ry>
      <rz>0.0</rz>
    </transmitteroffset>

    <notrackingposition>
      <x>0.0</x>
      <y>1.2</y>
      <z>0.6</z>
    </notrackingposition>

    <platformpos>
      <x>-2.0</x>
      <y>0.0</y>
      <z>12.0</z>
    </platformpos>

    <platformrot>
      <yaw>0.0</yaw>
    </platformrot>

    <groundfollowing>
      <activated>True</activated>
      <raystartheight>0.75</raystartheight>
    </groundfollowing>

    <movementtraces>False</movementtraces>

  </device>


  <!-- First User -->

  <user>

    <vip>False</vip>

    <startplatform>0</startplatform>

    <glasses>1</glasses>

    <headtrackingstation>tracking-lcd-glasses-1</headtrackingstation>

    <eyedist>0.065</eyedist>

    <warnings>False</warnings>

  </user>

  <user>

    <vip>False</vip>

    <startplatform>0</startplatform>

    <glasses>2</glasses>

    <headtrackingstation>tracking-lcd-glasses-2</headtrackingstation>

    <eyedist>0.065</eyedist>

    <warnings>False</warnings>

  </user>


</setup>
